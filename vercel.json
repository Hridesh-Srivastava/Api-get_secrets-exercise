{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["views/**", "public/**", "node_modules/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/styles/(.*)",
        "dest": "/public/styles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  
  